                   </rect>
                  </property>
                  <property name="text">
                   <string>合同注意事项</string>
                  </property>
                 </widget>
                 <widget class="QTextBrowser" name="textBrowser">
                  <property name="geometry">
                   <rect>
                    <x>10</x>
                    <y>40</y>
